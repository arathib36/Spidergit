#include<bits/stdc++.h>

using namespace std;

int main()
{int min,j,arr[10],i,n,temp;
cin>>n;
for(i=0;i<n;i++)
{cin>>arr[i];}

for(i=0;i<n;i++)
 {min=arr[i];
  
  for(j=i+1; j<n; j++)
   {
	if(arr[i]>arr[j])
    {
        temp=arr[i];
	arr[i]=arr[j];
	arr[j]=temp;
			}
}	}

for(i=0;i<n;i++)
{cout<<arr[i];
}

}
